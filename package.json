{
  "name": "grimace",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/grimace/grimace.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "lint": "eslint --cache --report-unused-disable-directives \"./**/*.{js,ts,tsx}\"",
    "start": "stencil build --dev --watch --serve",
    "pretest": "yarn generate-facedata-validator",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "generate-facedata-validator": "typescript-json-validator src/facedata.d.ts Facedata"
  },
  "dependencies": {
    "@stencil/core": "^2.11.0"
  },
  "devDependencies": {
    "@stencil/store": "^1.5.0",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.5.1",
    "puppeteer": "^10.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "typescript": "^4.5.4",
    "typescript-json-validator": "^2.4.2"
  },
  "license": "MIT"
}
